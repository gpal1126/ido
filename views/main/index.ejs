<script type="text/javascript">
$(function() {
var count = $('#rank-list li').length;
var height = $('#rank-list li').height();

function step(index) {
  // console.log(index);
  if(index==0){
    setTimeout(function() {
      $('ol').css('top',0);
        $('#rank-list ol').delay(800).animate({
        top: -height * index,
        }, 500, function() {
        step((index + 1) % count);
        });
    }, 1000);
  }else{
    $('#rank-list ol').delay(800).animate({
    top: -height * index,
    }, 500, function() {
    step((index + 1) % count);
    });  

  }
      
}

step(1);
});
</script>

