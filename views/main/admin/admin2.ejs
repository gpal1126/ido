<script type="text/javascript">
  $(function () {

    $(".tab_content").hide();
    $(".tab_content:first").show();

    $("ul.tabs li").click(function () {
        $("ul.tabs li").removeClass("active").css("color", "#333");
        //$(this).addClass("active").css({"color": "darkred","font-weight": "bolder"});
        $(this).addClass("active").css("color", "#f9f9fb");
        $(".tab_content").hide()
        var activeTab = $(this).attr("rel");
        $("#" + activeTab).fadeIn()
    });
});
</script>
<style type="text/css">
  ul.tabs {
    margin: 0;
    padding: 0;
    float: left;
    list-style: none;
    height: 32px;
    border-left: 1px solid #eee;
    width: 100%;
    font-family:"dotum";
    font-size:12px;
}
ul.tabs li {
    float: left;
    text-align:center;
    cursor: pointer;
    width:170px;
    height: 40px;
    line-height: 40px;
    border: 1px solid #eee;
    border-left: none;
    font-weight: normal;
    background: #ffffff;
    overflow: hidden;
    position: relative;
}
ul.tabs li.active {
    background: #4e92c1;
    border-bottom: 2px solid #f9f9fb;
}
.tab_container {
    border: 1px solid #eee;
    clear: both;
    float: left;
    width: 1100px;
    background: #FFFFFF;
    margin-top: 9px;
}
.tab_content {
    padding: 5px;
    font-size: 17px;
    display: none;
}
.tab_container .tab_content ul {
    width:100%;
    margin:0px;
    padding:0px;
}
.tab_container .tab_content ul li {
    padding:5px;
    list-style:none
}
;
 #admin_container {
    width: 249px;
    height: auto;
    top: 100px;
    position: relative;
}

tbody[id*=admin_tb]{
  font-size: 12px;
  font-weight: normal;
}

tbody[id*=admin_tb] tr{
  height: 30px;
}
</style>
</head>
<body>

<div id="admin_container" style="width: auto; height: auto; margin: 100px 0 0 20%">
