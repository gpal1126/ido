   $('#request_login').click(function(){
   alert('로그인이 필요합니다');
   	var result = confirm('로그인 하시겠습니까?');
        if(result) {
            location.href= '/ido_login';
        } else {
            //no
        }
   });
    
   $('.surveyWrap').on('click', '.closeButton',function(){ //설문 닫기시 
	   	$('body').off('scroll touchmove mousewheel');
   		$('.top_move_btn').css('display','')
	   	//$('html, body').css({'overflow': 'scroll', 'height':'100%'});	
   		reload_main();
   });
   
   window.addEventListener('load', function(){
	    document.body.style.height = (document.documentElement.clientHeight + 5) + 'px';
	    window.scrollTo(0, 1);
	}, false);
   
   function reload_main(){
   	   $('.surveyWrap').empty();
       $('.surveyWrap').css('display','none'); 
       $('.surveyWrap1').css('display','none');
       $('.survey_category_button').css('display','')
       $('.header').css({'position':'fixed'});	//헤더 고정
       $('body').css('position','');	//바디 고정 풀기
       $('.flexslider').css('padding-top','45px');	//상단 배너 padding-top
   }
	$('.conDiv').css('height', height);
	
	//검색 클릭
	$('.open_search').click(function(){
		console.log('검색 클릭');
		$(".search_wrap").animate({width:'toggle'}, 350); 
	});
	
	//검색어 검색
	$('#searchBtn').click(function(e){
		var searchTxt = $('#searchInput').val();
		
		if(searchTxt == ''){
			alert('검색어를 입력해주세요.');
		  	return false;
		}//if
		
		//기존 컨텐츠 지우기
		$('#container .left').empty();
		$('#container .right').empty();
		
		//category 값 초기화
		var category = $('#category').val('');
		console.log('category::'+category);
		$('#search').val(searchTxt);
		
		//컨텐츠 가져오기 function (category 부분 ''로 넘김) - 위의 category 값으로 넘기면 object로 넘어가서 안됨 
		call_image(0, '', searchTxt);	
		
	});//end click
	
});//end ready
</script>
